load("@io_bazel_rules_go//go:def.bzl", "go_library")
load("@io_bazel_rules_go//proto:def.bzl", "go_proto_library")

package(default_visibility = ["//visibility:public"])

proto_library(
    name = "internal_proto",
    srcs = ["stream_chunk.proto"],
    deps = ["@com_google_protobuf//:any_proto"],
)

go_proto_library(
    name = "internal_go_proto",
    importpath = "github.com/grpc-ecosystem/grpc-gateway/internal",
    proto = ":internal_proto",
)

go_library(
    name = "go_default_library",
    srcs = ["stream_chunk.pb.go"],
    importmap = "sigs.k8s.io/cluster-api-provider-aws/vendor/github.com/grpc-ecosystem/grpc-gateway/internal",
    importpath = "github.com/grpc-ecosystem/grpc-gateway/internal",
    deps = [
        "//vendor/github.com/golang/protobuf/proto:go_default_library",
        "//vendor/github.com/golang/protobuf/ptypes/any:go_default_library",
    ],
)
